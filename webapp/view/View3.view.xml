										</FlexBox>
									</l:content>
								</l:Grid>
							</IconTabFilter>
							<IconTabFilter text="النشا التام">
								<l:Grid>
									<l:content>
										<FlexBox direction="Column" alignItems="Center">
											<Label class="margin_right" labelFor="orderNo1" text="Order Number"/>
											<Input id="orderNo1"/>
											<layoutData>
												<l:GridData span="L12 M12 S12"/>
											</layoutData>
										</FlexBox>
										<FlexBox justifyContent="SpaceAround" wrap="Wrap">
											<FlexBox direction="Column" alignItems="Center">
												<Label class="margin_right" labelFor="K" text="النشا المعبأ (K)"/>
												<Input id="K"/>
												<layoutData>
													<l:GridData span="L4 M4 S12"/>
												</layoutData>
											</FlexBox>
											<FlexBox direction="Column" alignItems="Center">
												<Label class="margin_right" labelFor="L" text="إرتجاع الإنتاج (L)"/>
												<Input id="L"/>
												<layoutData>
													<l:GridData span="L4 M4 S12"/>
												</layoutData>
											</FlexBox>
											<FlexBox direction="Column" alignItems="Center">
												<Label class="margin_right" labelFor="M" text="فروق أوزان المبيعات (M)"/>
												<Input id="M"/>
												<layoutData>
													<l:GridData span="L4 M4 S12"/>
												</layoutData>
											</FlexBox>
											<layoutData>
												<l:GridData span="L12 M12 S12"/>
											</layoutData>
										</FlexBox>
									</l:content>
								</l:Grid>
							</IconTabFilter>
							<IconTabFilter text="فرق تغير تانكات حليب النشا">
								<l:Grid>
									<l:content>
										<FlexBox direction="Column" alignItems="Center">
											<Label class="margin_right" labelFor="orderNo2" text="Order Number"/>
											<Input id="orderNo2"/>
											<layoutData>
												<l:GridData span="L12 M12 S12"/>
											</layoutData>
										</FlexBox>
										<FlexBox justifyContent="SpaceAround">
											<FlexBox direction="Column" alignItems="Center">
												<Label class="margin_right" labelFor="T1" text="قياس آخر الوردية تانك 1"/>
												<Input id="T1"/>
												<layoutData>
													<l:GridData span="L6 M6 S12"/>
												</layoutData>
											</FlexBox>
											<FlexBox direction="Column" alignItems="Center">
												<Label class="margin_right" labelFor="KM" text="الكسب المعبأ"/>
												<Input id="KM"/>
												<layoutData>
													<l:GridData span="L6 M6 S12"/>
												</layoutData>
											</FlexBox>
											<layoutData>
												<l:GridData span="L12 M12 S12"/>
											</layoutData>
										</FlexBox>
										<FlexBox justifyContent="SpaceAround">
											<FlexBox direction="Column" alignItems="Center">
												<Label class="margin_right" labelFor="T2" text="قياس آخر الوردية تانك 2"/>
												<Input id="T2"/>
												<layoutData>
													<l:GridData span="L6 M6 S12"/>
												</layoutData>
											</FlexBox>
											<FlexBox alignItems="Center" direction="Column">
												<Label class="margin_right" labelFor="GM" text="جلوتوفيد معبأ"/>
												<Input id="GM"/>
												<layoutData>
													<l:GridData span="L6 M6 S12"/>
												</layoutData>
											</FlexBox>
											<layoutData>
												<l:GridData span="L12 M12 S12"/>
											</layoutData>
										</FlexBox>
										<FlexBox justifyContent="SpaceAround">
											<FlexBox direction="Column" alignItems="Center">
												<Label class="margin_right" labelFor="T3" text="قياس آخر الوردية تانك 3"/>
												<Input id="T3"/>
												<layoutData>
													<l:GridData span="L6 M6 S12"/>
												</layoutData>
											</FlexBox>
											<FlexBox alignItems="Center" direction="Column">
												<Label class="margin_right" labelFor="JM" text="جلوتين معبأ"/>
