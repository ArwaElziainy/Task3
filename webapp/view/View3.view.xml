											</FlexBox>
											<layoutData>
												<l:GridData span="L12 M12 S12"/>
											</layoutData>
										</FlexBox>
									</l:content>
								</l:Grid>
								<FlexBox class="body" alignItems="Center" justifyContent="Center" direction="Column">
									<Label class="margin_right" labelFor="first" text="عداد أول (مياه النقع)"/>
									<Input id="first"/>
									<Label class="margin_right" labelFor="last" text="عداد أخر (مياه النقع)"/>
