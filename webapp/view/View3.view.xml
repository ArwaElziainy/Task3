<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	xmlns:l="sap.ui.layout" controllerName="Task3.controller.View3" displayBlock="true">
	<App>
		<pages>
			<Page title="{i18n>title}" showNavButton="true" navButtonPress=".onNavBack">
				<HeaderContainer>
						<Button class="editIcon" icon="sap-icon://create-form" tooltip="Create" press=".onCreate"/>
				</HeaderContainer>
				<content>
					
					<IconTabBar showOverflowSelectList="true">
						<items>
							<IconTabFilter text="General Information">
							
								<l:Grid>
									<l:content>
										<FlexBox alignItems="Center" direction="Column">
											<Label class="margin_right" labelFor="shift" text="Shift"/>
											<ComboBox id="shift">
												<core:Item key="1" text="1"/>
												<core:Item key="2" text="2"/>
												<core:Item key="3" text="3"/>
											</ComboBox>
											<layoutData>
												<l:GridData span="L6 M6 S12"/>
											</layoutData>
										</FlexBox>
										<FlexBox alignItems="Center" direction="Column">
											<Label class="margin_right" labelFor="date" text="Date"/>
											<Input id="date"/>
											<layoutData>
												<l:GridData span="L6 M6 S12"/>
											</layoutData>
										</FlexBox>
										
									</l:content>
								</l:Grid>
							</IconTabFilter>
							<IconTabFilter text="تحويلات الجلوكوز">
								<FlexBox class="container" wrap="Wrap">
									<l:Grid>
										<l:content>
											<FlexBox alignItems="Center" direction="Column">
												<Label class="margin_right" labelFor="awl1" text="عداد أول الوردية 1"/>
												<Input id="awl1"/>
												<layoutData>
													<l:GridData span="L4 M4 S12"/>
												</layoutData>
											</FlexBox>
											<FlexBox alignItems="Center" direction="Column">
												<Label class="margin_right" labelFor="akher1" text="عداد آخر الوردية 1"/>
												<Input id="akher1"/>
												<layoutData>
													<l:GridData span="L4 M4 S12"/>
												</layoutData>
											</FlexBox>
											<FlexBox alignItems="Center" direction="Column">
												<Label class="margin_right" labelFor="y1" text="بومية 1"/>
												<Input id="y1"/>
												<layoutData>
													<l:GridData span="L4 M4 S12"/>
												</layoutData>
											</FlexBox>
											<FlexBox alignItems="Center" direction="Column">
												<Label class="margin_right" labelFor="awl2" text="عداد أول الوردية 2"/>
												<Input id="awl2"/>
												<layoutData>
													<l:GridData span="L4 M4 S12"/>
												</layoutData>
											</FlexBox>
											<FlexBox alignItems="Center" direction="Column">
												<Label class="margin_right" labelFor="akher2" text="عداد آخر الوردية 2"/>
												<Input id="akher2"/>
												<layoutData>
													<l:GridData span="L4 M4 S12"/>
												</layoutData>
											</FlexBox>
											<FlexBox alignItems="Center" direction="Column">
												<Label class="margin_right" labelFor="y2" text="بومية 2"/>
												<Input id="y2"/>
												<layoutData>
													<l:GridData span="L4 M4 S12"/>
												</layoutData>
											</FlexBox>
											<FlexBox alignItems="Center" direction="Column">
												<Label class="margin_right" labelFor="br" text="نسبة البروتين"/>
												<Input id="br"/>
												<layoutData>
													<l:GridData span="L4 M4 S12"/>
												</layoutData>
											</FlexBox>
										</l:content>
									</l:Grid>
								</FlexBox>
								<FlexBox alignItems="Center" class="container" direction="Column">
									<Label class="margin_right" labelFor="total" text="إجمالى التحويل للفركتوز"/>
									<Input id="total"/>
								</FlexBox>
							</IconTabFilter>
							<IconTabFilter text="النشــا الجاف">
								<l:Grid>
									<l:content>
										<FlexBox alignItems="Center" direction="Column">
											<Label class="margin_right" labelFor="orderNo" text="Order Number"/>
											<Input id="orderNo"/>
											<layoutData>
												<l:GridData span="L12 M12 S12"/>
											</layoutData>
										</FlexBox>
										<FlexBox justifyContent="SpaceAround">
											<FlexBox alignItems="Center" direction="Column">
												<Label class="margin_right" labelFor="H" text="قياس تانك النشا الجاف أول الوردية (H)"/>
												<Input id="H"/>
												<layoutData>
													<l:GridData span="L6 M6 S12"/>
												</layoutData>
											</FlexBox>
											<FlexBox alignItems="Center" direction="Column">
												<Label class="margin_right" labelFor="I" text="قياس تانك النشا الجاف آخر الوردية (I)"/>
