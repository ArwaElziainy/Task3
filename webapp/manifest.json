{
	"_version": "1.7.0",
	"sap.app": {
		"id": "Task3",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "Task3.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "Task3.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "Task3.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [{
				"pattern": "",
				"name": "View1",
				"target": "View1"
			}, {
				"pattern": "{refNo}",
				"name": "View2",
				"target": "View2"
			}, {
				"pattern": "View3",
				"name": "View3",
				"target": "View3"
			}],
			"targets": {
				"View1": {
					"viewType": "XML",
					"viewName": "View1",
					"viewLevel": 0,
					"controlAggregation": "pages"
				},
				"View2": {
					"viewName": "View2",
					"viewType": "XML",
					"transition": "slide",
					"controlAggregation": "pages",
					"viewLevel": 1
				},
				"View3": {
					"viewName": "View3",
					"viewType": "XML",
					"transition": "slide",
					"controlAggregation": "pages",
					"viewLevel": 2
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}